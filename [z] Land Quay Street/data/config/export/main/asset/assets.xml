			  </AdaptToNeighbors>
			</Street>
			<Constructable>
			  <DestructionCategory>StreetsAndBridges</DestructionCategory>
			</Constructable>
			<Locked/>
			<SoundEmitter>
			  <DestroySounds>
				<Item>
				  <Sound>200777</Sound>
				</Item>
			  </DestroySounds>
			  <ConstructionSounds>
				<BuildSuccessful>
				  <Item>
					<Sound>215917</Sound>
				  </Item>
				</BuildSuccessful>
			  </ConstructionSounds>
			  <ConstructionStreetSounds>
				<BuildAddWaypoint>
				  <Item>
					<Sound>215920</Sound>
				  </Item>
				</BuildAddWaypoint>
			  </ConstructionStreetSounds>
			  <MaterialType>Street_Brick</MaterialType>
			</SoundEmitter>
			<Text>
			  <LocaText>
				<English>
				  <Text>Paved Street</Text>
				  <Status>Exported</Status>
				  <ExportCount>2</ExportCount>
				</English>
			  </LocaText>
			  <LineID>4224</LineID>
			</Text>
			<Building>
			  <AssociatedRegions>Moderate;Colony01</AssociatedRegions>
			</Building>
		  </Values>
		</Asset>
	  </ModOp>

